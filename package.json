{
  "name": "transfer",
  "version": "0.0.1",
  "description": "An HTTP/HTTPS proxy server for debugging",
  "scripts": {
    "build": "babel ./src --out-dir build",
    "dev": "babel ./src --watch --out-dir build",
    "test": "tape test/**/*.js | tap-notify | tap-diff",
    "test-utils": "tape test/utils/*.js | tap-notify | tap-diff",
    "test-server": "node test/server.js"
  },
  "author": "ohmyxm",
  "license": "ISC",
  "dependencies": {
    "cert-base": "^0.1.1",
    "chalk": "^1.1.3",
