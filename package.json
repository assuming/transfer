    "koa": "^2.0.0",
    "koa-router": "^7.0.1",
    "superagent": "^3.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "tape": "^4.6.3"
  },
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assuming/transfer.git"
